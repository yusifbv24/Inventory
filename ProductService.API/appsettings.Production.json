{
    "AllowedHosts": "*",
    "ImageSettings": {
        "Path": "wwwroot/images/products",
        "MaxSizeInMB": 5,
        "BaseUrl": "https://inventory166.az/images/products"
    },
    "RabbitMQ": {
        "HostName": "rabbitmq"
    },
    "Jwt": {
        "ExpirationInMinutes": 480
    },
    "Services": {
        "ApprovalService": "http://api-gateway",
        "RouteService": "http://route-service"
    },
    "Serilog": {
        "MinimumLevel": {
            "Default": "Warning",
            "Override": {
                "Microsoft": "Warning",
                "Microsoft.AspNetCore": "Warning",
                "Microsoft.AspNetCore.Authentication": "Warning",
                "System.Net.Http.HttpClient": "Warning"
            }
        },
        "WriteTo": [
            {
                "Name": "Seq",
                "Args": {
                    "serverUrl": "http://seq:80",
                    "restrictedToMinimumLevel": "Warning"
                }
            }
        ]
    }
}
