{
  "AllowedHosts": "*",
  "ImageSettings": {
    "Path": "wwwroot/images/products",
    "MaxSizeInMB": 5
  },
  "RabbitMQ": {
    "HostName": "rabbitmq"
  },
  "Jwt": {
    "ExpirationInMinutes": 480
  },
  "Services": {
    "RouteService": "http://route-service"
  },
  "Serilog": {
    "MinimumLevel": {
      "Default": "Warning",
      "Override": {
        "Microsoft": "Warning",
        "Microsoft.AspNetCore": "Warning"
      }
    }
  }
}
