{
    "Logging": {
        "LogLevel": {
            "Default": "Debug",
            "Microsoft.AspNetCore": "Information",
            "RabbitMQ": "Debug"
        }
    },
    "AllowedHosts": "*",
    "ProductService": {
        "BaseUrl": "http://product-service"
    },
    "RabbitMQ": {
        "HostName": "rabbitmq",
        "ExchangeName": "inventory-events"
    },
    "ImageSettings": {
        "Path": "wwwroot/images/routes",
        "MaxSizeInMB": 5
    },
    "Jwt": {
        "ExpirationInMinutes": 480
    }
}